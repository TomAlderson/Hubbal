<%= page("calendar","Timetables","Your personal timetables.") %>

<div class="row">

  <% @timetables.each do |timetable| %>
    <%= link_to(timetable) do %>
      <div class="col-md-4">
        <div class="box">
          <div class="box-header with-border">
            <h3 class="box-title"><i class="fa fa-calendar-o"></i></h3>
          </div>
          <div class="box-body">
            <table class="table table-striped">
              <thead>
                <tr></tr>
              </thead>
              <tbody>
                <tr>
                  <td><strong>Average Hours Per Day</strong></td>
                  <td>10</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    <% end %>
  <% end %>

</div>
